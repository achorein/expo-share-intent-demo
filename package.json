{
  "name": "expo-share-intent-demo",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "clean": "expo prebuild --clean && patch -s -p0 < plugins/share-extension-patch-android.diff",
    "prebuild": "expo prebuild --no-install && patch -s -p0 < plugins/share-extension-patch-android.diff",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "test": "CI=1 expo install --check",
    "test:fix": "expo install --fix",
    "doctor": "npx expo-doctor",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "expo": "~46.0.21",
    "expo-build-properties": "~0.3.0",
    "expo-config-plugin-ios-share-extension": "^0.0.4",
    "expo-constants": "~13.2.4",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.0.0",
    "react-native": "0.69.9",
    "react-native-receive-sharing-intent": "^2.0.0",
    "react-dom": "18.0.0",
    "react-native-web": "~0.18.7"
  },
  "resolutions": {
    "@expo/config-plugins": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6"
  },
  "private": true
}
